<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Button mnemonicParsing="false" prefHeight="30.0" prefWidth="114.0" text="&lt; Utilisateurs">
         <font>
            <Font name="Arial Black" size="13.0" />
         </font>
      </Button>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
         <children>
            <VBox alignment="CENTER" fillWidth="false" prefHeight="300.0" prefWidth="100.0" spacing="10.0" HBox.hgrow="ALWAYS">
               <children>
                  <TextField alignment="CENTER" prefHeight="25.0" prefWidth="197.0" promptText="Adresse mail ou nom d'utilisateur">
                     <font>
                        <Font name="Arial Italic" size="12.0" />
                     </font>
                  </TextField>
                  <PasswordField alignment="CENTER" prefHeight="25.0" prefWidth="197.0" promptText="Mot de passe " />
                  <VBox alignment="BOTTOM_CENTER" prefHeight="121.0" prefWidth="320.0">
                     <children>
                        <Button alignment="CENTER" mnemonicParsing="false" text="Connexion ">
                           <font>
                              <Font name="Arial Black" size="14.0" />
                           </font>
                        </Button>
                     </children>
                  </VBox>
               </children>
            </VBox>
            <VBox alignment="CENTER" fillWidth="false" prefHeight="200.0" prefWidth="100.0" spacing="10.0" HBox.hgrow="ALWAYS">
               <children>
                  <TextField alignment="CENTER" prefHeight="25.0" prefWidth="197.0" promptText="Adresse mail ">
                     <font>
                        <Font name="Arial Italic" size="12.0" />
                     </font>
                     <cursor>
                        <Cursor fx:constant="SW_RESIZE" />
                     </cursor>
                  </TextField>
                  <TextField alignment="CENTER" prefHeight="25.0" prefWidth="197.0" promptText="Nom d'utilisateur">
                     <font>
                        <Font name="Arial Italic" size="12.0" />
                     </font>
                     <cursor>
                        <Cursor fx:constant="SW_RESIZE" />
                     </cursor>
                  </TextField>
                  <PasswordField alignment="CENTER" prefHeight="25.0" prefWidth="197.0" promptText="Mot de passe ">
                     <cursor>
                        <Cursor fx:constant="SW_RESIZE" />
                     </cursor>
                  </PasswordField>
                  <PasswordField alignment="CENTER" prefHeight="25.0" prefWidth="197.0" promptText="Confirmez votre mot de passe">
                     <cursor>
                        <Cursor fx:constant="SW_RESIZE" />
                     </cursor>
                  </PasswordField>
                  <VBox alignment="BOTTOM_CENTER" prefHeight="51.0" prefWidth="320.0">
                     <children>
                        <Button alignment="CENTER" mnemonicParsing="false" text="Inscription">
                           <font>
                              <Font name="Arial Black" size="14.0" />
                           </font>
                           <cursor>
                              <Cursor fx:constant="SW_RESIZE" />
                           </cursor>
                        </Button>
                     </children>
                  </VBox>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</VBox>
